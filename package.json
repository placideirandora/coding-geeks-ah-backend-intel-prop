{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node ./src/index.js",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "build": "rm -rf build && babel -d ./build ./src -s",
    "start": "npm run build && npm run migrate && npm run seed && node ./build/index.js",
    "pretest": "cross-env NODE_ENV=test npm run down && cross-env NODE_ENV=test npm run migrate  && cross-env NODE_ENV=test npm run seed",
    "test": "cross-env NODE_ENV=test nyc --reporter=text mocha  --require @babel/register --require @babel/polyfill  ./src/tests/*.js --timeout 50000 --exit",
    "down": "node_modules/.bin/sequelize db:migrate:undo:all",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "seed": "node_modules/.bin/sequelize db:seed:all",
    "drop:db": "cross-env NODE_ENV=test node_modules/.bin/sequelize db:drop authors_haven_test",
    "create:db": "cross-env NODE_ENV=test node_modules/.bin/sequelize db:create authors_haven_test"
  },
  "engines": {
    "node": "10.16.0"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "arr-pluck": "^0.1.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.14.0",
    "connect-multiparty": "^2.2.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.1",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-session": "^1.16.2",
    "http-status-code": "^2.1.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.3.0",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "mongoose": "^5.2.2",
    "mongoose-unique-validator": "^2.0.1",
    "morgan": "^1.9.1",
    "open": "^6.4.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^7.12.1",
    "sequelize": "^5.12.0",
    "sequelize-cli": "^5.5.0",
    "slug": "^1.1.0",
    "socket.io": "^2.2.0",
    "swagger-ui-express": "^4.0.7",
    "underscore": "^1.9.1",
    "uniqid": "^5.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  }
}
